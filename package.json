{
  "name": "angular-webworker",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "run-s start-webworker",
    "build": "run-s build-webworker",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",

    "build-browser": "ng build --prod",
    "start-browser": "ng serve",

    "build-server": "run-s build build-server:clean build-server:ngcli build-server:webpack",
    "build-server:clean": "shx rm -rf dist/server && shx mkdir -p dist/server",
    "build-server:ngcli": "ng build --app server --aot --output-hashing=none --output-path dist/server",
    "build-server:webpack": "webpack --config webpack.server.config.js --progress --colors",
    "start-server": "run-s build-server start-server:express",
    "start-server:express": "node dist/server",

    "build-webworker": "run-s build-webworker:clean build-webworker:webpack",
    "build-webworker:clean": "shx rm -rf dist/webworker && shx mkdir -p dist/webworker",
    "build-webworker:webpack": "webpack --config webpack.webworker.config.js --progress --colors",
    "start-webworker": "run-s build-webworker:clean start-webworker:webpack",
    "start-webworker:webpack": "webpack-dev-server --config webpack.webworker.config.js --port=4200"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/cdk": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/material": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/platform-server": "6.0.0",
    "@angular/platform-webworker": "6.0.0",
    "@angular/platform-webworker-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "@nguniversal/express-engine": "6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "6.0.0",
    "core-js": "^2.4.1",
    "font-awesome": "4.7.0",
    "ngx-bootstrap": "2.0.4",
    "rxjs": "6.1.0",
    "rxjs-compat": "6.1.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@ng-bootstrap/ng-bootstrap": "2.0.0",
    "@types/jasmine": "~2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "10.0.4",
    "bootstrap": "4.1.1",
    "codelyzer": "^4.0.1",
    "express": "4.16.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "4.1.2",
    "protractor": "~5.3.1",
    "shx": "0.2.2",
    "ts-node": "6.0.2",
    "tslint": "5.10.0",
    "typescript": "2.7.2"
  }
}
